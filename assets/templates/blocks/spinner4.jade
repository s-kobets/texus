//spinner
.input-group
	span.input-group-btn
		button.btn.btn-default(type='button', data-value='decrease', data-target='#spinner4', data-toggle='spinner')
			span.glyphicon.glyphicon-chevron-left
	input#spinner4.form-control.input-number(type='text', data-ride='spinner', value='1')
	span.input-group-btn
		button.btn.btn-default(type='button', data-value='increase', data-target='#spinner4', data-toggle='spinner')
			span.glyphicon.glyphicon-chevron-right
